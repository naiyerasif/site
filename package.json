    "@noxify/gridsome-plugin-remark-embed": "^1.3.1",
    "axios": "^0.21.2",
    "fuse.js": "^6.4.6",
    "gridsome": "^0.7.23",
    "gridsome-remark-figure-caption": "^1.2.0"
  },
  "devDependencies": {
    "@microflash/spritely": "^1.3.3",
    "@sindresorhus/slugify": "^0.8.0",
    "autoprefixer": "^9.8.6",
    "commander": "^8.1.0",
    "dayjs": "^1.10.6",
    "gray-matter": "^4.0.3",
    "keyword-extractor": "^0.0.20",
    "marked": "^3.0.2",
